{
  "buildCommand": "npm run build",
  "outputDirectory": ".next",
  "framework": "nextjs",
  "regions": ["icn1"],
  "functions": {
    "app/api/**": {
      "maxDuration": 30
    }
  },
  "crons": [
    {
      "path": "/api/backup/cron",
      "schedule": "0 2 * * *"
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false
}